  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#7a7a8a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#d0d0dd"
      }
    },
    {
      "scope": [
        "variable.language"
      ],
      "settings": {
        "foreground": "#b899c9"
      }
    },
    {
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#c9b19d"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#d4c49d"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#9db899"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#a89bc7"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#9d9daf"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#b899c9"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#9d9dc9"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#b8a99d"
      }
    },
    {
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#a89bc7"
      }
    },
    {
      "scope": [
        "variable.parameter",
        "meta.function.parameters"
      ],
      "settings": {
        "foreground": "#c9b19d"
      }
    },
    {
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#a89bc7"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#9d9dc9"
      }
    },
    {
      "scope": [
        "support.constant",
        "support.variable"
      ],
      "settings": {
        "foreground": "#b899c9"
      }
    },
    {
      "scope": [
        "meta.import",
        "meta.export"
      ],
      "settings": {
        "foreground": "#9d9dc9"
      }
    },
    {
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#a89bc7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#b899c9",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#9d9dc9",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.list"
      ],
      "settings": {
        "foreground": "#9db899"
      }
    },
    {
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#7a7a8a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code"
      ],
      "settings": {
        "foreground": "#9db899"
      }
    },
    {
      "scope": [
        "punctuation"
      ],
      "settings": {
        "foreground": "#9d9daf"
      }
    },
    {
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#c9919e"
      }
    },
    {
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#d4c49d"
      }
    }
  ]
}
