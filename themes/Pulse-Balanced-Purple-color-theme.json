  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6a6a7e",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#d5d5e3"
      }
    },
    {
      "scope": [
        "variable.language"
      ],
      "settings": {
        "foreground": "#c49ad8"
      }
    },
    {
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#d4b8a3"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#ddc8a3"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#a8c99a"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#9d7dc7"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#b5b5ca"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#c49ad8"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#9d9ddb"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#d4b8a3"
      }
    },
    {
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#9d7dc7"
      }
    },
    {
      "scope": [
        "variable.parameter",
        "meta.function.parameters"
      ],
      "settings": {
        "foreground": "#d4b8a3"
      }
    },
    {
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#9d7dc7"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#9d9ddb"
      }
    },
    {
      "scope": [
        "support.constant",
        "support.variable"
      ],
      "settings": {
        "foreground": "#c49ad8"
      }
    },
    {
      "scope": [
        "meta.import",
        "meta.export"
      ],
      "settings": {
        "foreground": "#9d9ddb"
      }
    },
    {
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#9d7dc7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#c49ad8",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#9d9ddb",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.list"
      ],
      "settings": {
        "foreground": "#a8c99a"
      }
    },
    {
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#6a6a7e",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code"
      ],
      "settings": {
        "foreground": "#a8c99a"
      }
    },
    {
      "scope": [
        "punctuation"
      ],
      "settings": {
        "foreground": "#b5b5ca"
      }
    },
    {
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#d89aaa"
      }
    },
    {
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#ddc8a3"
      }
    }
  ]
}
